    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "drizzle-kit": "^0.20.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "postcss": "^8",
